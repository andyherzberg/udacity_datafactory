{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adlsnycpayroll-data-factory"
		},
		"GitHub_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub'"
		},
		"ls_azure_datalake_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azure_datalake_gen2'"
		},
		"ls_sql_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_db'"
		},
		"GitHub_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "andyherzberg@yahoo.de"
		},
		"ls_azure_datalake_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapseaccadlsnycpayroll.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adlsnycpayroll-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GitHub_credential": {
						"value": "[parameters('GitHub_credential')]"
					},
					"ls_azure_datalake_gen2_accountKey": {
						"value": "[parameters('ls_azure_datalake_gen2_accountKey')]"
					},
					"ls_sql_db_connectionString": {
						"value": "[parameters('ls_sql_db_connectionString')]"
					},
					"GitHub_properties_typeProperties_username": {
						"value": "[parameters('GitHub_properties_typeProperties_username')]"
					},
					"ls_azure_datalake_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_azure_datalake_gen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adlsnycpayroll-data-factory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adlsnycpayroll-data-factory_ArmTemplate_0')]"
			]
		}
	]
}